  agent any
  
  stages {
  
    stage("build"){
    
      steps {
        echo "building the application"
        echo "Application Built"
      }
    }
    
    stage("test"){
    
      steps {
        echo "Testing the application"
      }
    }
    
    stage("deeploy"){
    
      steps {
       echo "Deeploy the application"
      }
    }
    
  }

}
